{
  "manifest_version": 3,
  "name": "I Want Links on YouTube Video",
  "version": "1.0",
  "description": "Find and view links for YouTube videos directly from this extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "main-16.png",
      "48": "main-48.png",
      "128": "main-128.png"
    }
  },
  "icons": {
    "16": "main-16.png",
    "48": "main-48.png",
    "128": "main-128.png"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://*/*", "http://*/*"]
}

